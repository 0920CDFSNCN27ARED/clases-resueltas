<!DOCTYPE html>
<html lang="en">
<% const title="Actors List" %>

<head>
    <%- include("../partials/head.ejs", {title}) %>
</head>

<body>
    <div class="container mt-5">
        <%- include("../components/page-title.ejs", {title}) %>

        <ul>
            <% for( let i=0; i < actors.length; i++ ) { %>
            <li>
                <a href="/actors/<%= actors[i].id %>">
                    <%= getActorName(actors[i]) %>
                </a>
            </li>
            <% } %>
        </ul>
    </div>
</body>

</html>